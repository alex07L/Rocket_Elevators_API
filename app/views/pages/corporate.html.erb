								<strong>Thank You!</strong> Your message successfully sent!
							</div><!-- /Alert Success -->


							<!-- Alert Failed -->
							<div id="alert_failed" class="alert alert-danger mb-30">
								<button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
								<strong>[SMTP] Error!</strong> Internal server error!
							</div><!-- /Alert Failed -->


							<!-- Alert Mandatory -->
							<div id="alert_mandatory" class="alert alert-danger mb-30">
								<button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
								<strong>Sorry!</strong> You need to complete all mandatory (*) fields!
							</div><!-- /Alert Mandatory -->
                    <%= form_for :contact_us, url: url_for(controller: 'pages', action: 'contact_us_post'), :html =>{id: "sendMessage", :class => ""}, :multipart => true do |f|%>
								<fieldset>
									<input type="hidden" name="action" value="contact_send" />

									<div class="row">
										<div class="col-md-4">
											<%= f.label "Full Name *" %>
											<%= f.text_field :Full_Name, required: true, class: "form-control input-text elevators" %>
										</div>
						 					<div class="col-md-4">
											<%= f.label "Business Name *" %>
											<%= f.text_field :Business_Name, required: true, class: "form-control input-text elevators" %>
											</div>
										<div class="col-md-4">
											<%= f.label "E-mail Address *" %>
									        <%= f.text_field :Email, required: true, class: "form-control input-text elevators" %>
										</div>
                                        <div class="col-md-4">
											<%= f.label "Phone Number *" %>
											<%= f.text_field :Phone_Number, required: true, class: "form-control input-text elevators" %>
										</div>
										<div class="col-md-8">                                       									
											<%= f.label "Project Name *" %>
											<%= f.text_area :Project_Name, required: true, maxlength: "10000", class: "form-control input-text elevators" %>
										</div>
											
										<div class="col-md-12">
                                        <%= f.label "Description *" %>						
											<%= f.label " Projet Description *" %>
											<%= f.text_field :Description, required: true, class: "form-control input-text elevators" %>
										</div>
										<div class="col-md-4">									
											<%= f.label "Department *" %>
                                            <%= f.select(:Department, [['Residential', 'Residential'], ['Corporate', 'Corporate'], ['Commercial', 'Commercial']], {}, required: true, class: "form-control input-text elevators") %>
										</div>
										<div class="col-md-12">					
											<%= f.label "Message *" %>
											<%= f.text_area :Message, required: true, maxlength: "10000", rows: "8", class: "form-control input-text text-left elevators" %>
										</div>
										<div class="col-md-12">                                           
                                            <%= f.label "File Attachment" %>
								            <%= f.file_field 'File' %>
                                             <!-- custom file upload -->
											<small class="text-muted block">Max file size: 10Mb (zip/pdf/jpg/png)</small>

										</div>
									</div>
									
								</fieldset>

								<div class="row">
									<div class="col-md-12">
                                    <%= button_tag(class: "btn btn-primary") do %>
									<i class="fa fa-check"></i> SEND MESSAGE
								    <% end %>
										<button type="submit" class="btn btn-primary"><i class="fa fa-check"></i> SEND MESSAGE</button>
									</div>
